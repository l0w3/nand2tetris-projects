/**
 * 16-bit Or gate:
 * for i = 0, ..., 15:
 * out[i] = a[i] Or b[i] 
 */
CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
      Not16(in=a , out=na );
      Not16(in=b , out=nb );
      Nand16(a=na , b=nb , out=out );
}